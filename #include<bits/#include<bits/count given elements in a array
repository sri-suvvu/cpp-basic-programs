#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,num,count=0;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
      cin>>num;
      for(int i=0;i<n;i++)
      {
          if(num==arr[i])
          {
              count++;
          }
      }
      cout<<count;
}
    
