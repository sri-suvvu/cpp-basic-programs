#include<bits/stdc++.h>
using namespace std;
int main()
{
int num,i;
bool var=true;
    cin>>num;
    int arr[num];
    for(i=0;i<num;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<num;i++)
    {
        if(arr[i]%2!=0&&i%2==0)
        {
         var=false;
        }
        else 
        continue;
    }
    if(var)
     cout<<"True";
    else
     cout<<"False";
}
