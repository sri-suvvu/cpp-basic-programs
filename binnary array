#include<bits/stdc++.h>
using namespace std;
int main()
{
	int num;
	cin>>num;
	int i,count=0;
	int arr[num];
	for(i=0;i<num;i++)
	{
	cin>>arr[i];	
	}	
		for(i=0;i<num;i++)
		{
			if(arr[i]==0 || arr[i]==1)
			{
				continue;0
			}
			else
			  count++;
		}
		if(count>0)
		 cout<<"False";
		else 
		 cout<<"True";
}
