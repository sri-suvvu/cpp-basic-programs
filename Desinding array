#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    int i,j;
    bool val=true;
    cin>>n;
    int arr[n];
    for(i=0;i<n;i++)
     cin>>arr[i];
    for(i=0;i<n;i++)
    {
         for(j=i+1;j<n;j++)
         {
             if(arr[i]<arr[j])
             {
                val=false;
             }
             else 
              continue;
         }
         
    }
    if(val)
     cout<<"yes";
    else
     cout<<"no";
}
