#include<iostream>
using namespace std;
int main(){
 int n,m;
 cin>>m>>n;
 int arr[m][n];
 int sum=0;
 for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++){
        cin>>arr[i][j];
    }
 }
for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++){
      if (i == 0 || i == m - 1 || j == 0 || j == n - 1)
        sum=sum+arr[i][j];
}
}
cout<<sum;
}
